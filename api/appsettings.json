{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Urls": "http://*:5078",
  "RateLimiting": {
    "GlobalPolicy": {
      "PermitLimit": 100,
      "WindowInMinutes": 1
    },
    "ApiPolicy": {
      "PermitLimit": 10,
      "WindowInMinutes": 1,
      "QueueLimit": 5
    },
    "StrictPolicy": {
      "PermitLimit": 5,
      "WindowInMinutes": 1,
      "QueueLimit": 2
    }
  }
}
